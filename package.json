{
  "name": "eslint-plugin-clean-css-modules",
  "version": "0.0.1",
  "description": "Helps in writing code with css-modules",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "license": "ISC",
  "author": "Dmitry Zakharov",
  "main": "build/index.js",
  "scripts": {
    "dev": "rollup --config rollup.config.js --watch",
    "build": "rollup --config rollup.config.js",
    "test": "jest",
    "lint": "ESLINT_CLI=true eslint --cache",
    "lint:fix": "npm run lint -- --fix --ext .js,.jsx ./src",
    "prettify": "prettier --write",
    "pre-commit": "lint-staged && npm run test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre-commit"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run lint -- --fix"
    ],
    "*.{js,jsx,ts,tsx,md,html,css,scss,json}": [
      "npm run prettify --"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.13.8",
    "@babel/preset-env": "^7.13.8",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "rollup": "^2.40.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-terser": "^7.0.2"
  },
  "devDependencies": {
    "eslint": "^7.1.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-no-loops": "^0.3.0",
    "eslint-plugin-no-use-extend-native": "^0.5.0",
    "eslint-plugin-optimize-regex": "^1.2.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-sonarjs": "^0.5.0",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "mocha": "^7.2.0",
    "prettier": "^2.2.1"
  }
}
